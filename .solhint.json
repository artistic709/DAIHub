{
  "extends": "solhint:default",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "const-name-snakecase": "off",
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "avoid-tx-origin:": "warn",
    "not-rely-on-time": "off",
    "not-rely-on-block-hash": "warn"
  }
}