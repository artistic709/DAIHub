{
  "name": "dapp-contracts-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for dapp contract development",
  "main": "index.js",
  "scripts": {
    "compile": "npx buidler compile",
    "lint": "npx buidler check",
    "prettier": "prettier --write contracts/*.sol",
    "test": "npx buidler test",
    "test:rinkeby": "npx buidler --network rinkeby test",
    "deploy:local": "npx buidler run scripts/sample-script.js",
    "clean": "npx buidler clean",
    "flatten": "npx buidler flatten",
    "console": "npx buidler console"
  },
  "repository": {
    "type": "git",
    "url": "git@ssh.githubcom:Xiawpohr/dapp-contracts-boilerplate.git"
  },
  "keywords": [
    "Solidity",
    "Ethereun",
    "DApp"
  ],
  "author": "xiawpohr",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.0.2",
    "@nomiclabs/buidler-ethers": "^1.0.2",
    "@nomiclabs/buidler-solhint": "^1.0.2",
    "@nomiclabs/buidler-web3": "^1.0.2",
    "bignumber.js": "^9.0.0",
    "buidler-gas-reporter": "^0.1.3",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^2.3.0-istanbul.0",
    "ethers": "^4.0.41",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "prettier-plugin-solidity": "^1.0.0-alpha.35",
    "solhint": "^2.3.0",
    "solhint-plugin-prettier": "0.0.3",
    "web3": "^1.2.4"
  }
}
